		"ts-node": "^8.4.1",
		"ts-node-dev": "^1.0.0-pre.42",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.2.4",
		"typescript-tools": "^0.3.1"
	},
	"peerDependencies": {
		"@types/node": "^14.14.10"
	},
	"_moduleAliases": {
