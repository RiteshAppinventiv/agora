		"module-alias": "^2.2.2",
		"moment": "^2.29.1",
		"mongoose": "^5.11.0",
		"nodemailer": "^6.4.16",
		"nodemon": "^2.0.7",
		"npm": "^7.6.3",
		"pdfkit": "^0.13.0",
		"puppeteer": "^10.4.0",
		"random-int": "^2.0.1",
		"randomstring": "^1.1.5",
		"readable": "^1.1.3",
		"redis": "^2.8.0",
		"saslprep": "^1.0.3",
		"shopify-api-node": "^3.7.2",
		"tinyurl": "^1.1.5",
		"twilio": "^3.62.0",
