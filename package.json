{
	"name": "conkur",
	"version": "1.0.0",
	"description": "Node backend in Hapi.js and TypeScript.",
	"main": "./build/server.js",
	"engines": {
		"node": "14.15.1",
		"npm": "6.14.8"
	},
	"scripts": {
		"prestart": "tsc",
		"local": "tsc && NODE_ENV=local node ./build/server.js",
		"local:windows": " tsc && set NODE_ENV=local && node ./build/server.js",
		"default": "tsc && NODE_ENV=default node ./build/server.js",
		"start": "NODE_ENV=local nodemon --exec ts-node -- server.ts",
		"local-start": "NODE_ENV=local nodemon server.ts",
		"start:windows": "set NODE_ENV=local && nodemon --exec ts-node -- server.ts",
		"local:ts-node": "NODE_ENV=local ts-node -r tsconfig-paths/register server.ts --fast",
		"local:ts-node-dev": "NODE_ENV=local ts-node-dev -r tsconfig-paths/register server.ts --fast",
		"dev": "nodemon server.ts",
		"dev:debug": "nodemon --inspect server.ts",
		"pm2:dev": "tsc && NODE_ENV=development pm2 start ./build/server.js --name rcc-dev",
		"test": "mocha -r ts-node/register **/*.[sS]pec.ts",
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"tslint": "tslint --project tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"author": "Technologies",
	"license": "ISC",
	"dependencies": {
		"agora-access-token": "^2.0.4",
		"amqplib": "^0.7.0",
		"aws-sdk": "^2.802.0",
		"axios": "^0.27.2",
		"crypto-js": "^4.0.0",
		"csvtojson": "^2.0.10",
		"del": "^3.0.0",
		"dotenv": "^6.2.0",
		"fcm-node": "^1.5.2",
		"firebase-admin": "^9.8.0",
		"google-auth-library": "^7.0.4",
		"googleapi": "^1.0.2",
		"googleapis": "^67.1.1",
		"handlebars": "^4.7.6",
		"hapi-auth-basic": "^5.0.0",
		"hapi-auth-bearer-token": "^6.2.1",
		"hapi-i18n": "^3.0.0",
		"hapi-swagger": "^14.0.0",
		"html-pdf": "^3.0.1",
		"html-to-pdf": "^0.1.11",
		"i": "^0.3.6",
		"imap": "^0.8.19",
		"inert": "^5.1.3",
		"inspect": "0.0.2",
		"joi": "^17.2.1",
